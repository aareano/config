{
    "segments": [
        "battery",
        "time",
        "virtual_env",
        "aws_profile",
        "ssh",
        "cwd",
        "git",
        "git_stash",
        "jobs",
        "exit_code"
    ],
    "battery": {
        "always_show_percentage": true
    },
    "time": {
        "format": "%a %b %d  %I:%M %p"
    },
    "vcs": {
        "show_symbol": true
    },
    "cwd": {
        "mode": "plain",
        "max_depth": 4
    },
    "mode": "patched",
    "theme": "~/.config/powerline-shell/theme.py"
}
